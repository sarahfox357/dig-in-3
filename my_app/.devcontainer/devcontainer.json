{
  "name": "Flutter Codespace",
  "image": "mcr.microsoft.com/vscode/devcontainers/base:ubuntu-22.04",
  "postCreateCommand": "bash .devcontainer/install-flutter.sh",
  "features": {
    "git": "latest",
    "node": "latest"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ]
    }
  },
  "remoteUser": "vscode"
}
